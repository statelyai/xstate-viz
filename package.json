{
  "name": "xstate-viz-app",
  "version": "0.7.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@monaco-editor/react": "^4.2.1",
    "@replayio/cypress": "^0.2.3",
    "@sentry/react": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "@supabase/supabase-js": "^1.22.5",
    "@xstate/graph": "^1.3.0",
    "@xstate/inspect": "^0.4.1",
    "@xstate/react": "^1.5.1",
    "date-fns": "^2.22.1",
    "elkjs": "^0.7.1",
    "framer-motion": "^4",
    "immer": "^9.0.6",
    "lz-string": "^1.4.4",
    "memory-web-storage": "^1.0.0",
    "monaco-editor": "^0.25.2",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-markdown": "^6.0.2",
    "realms-shim": "^1.2.2",
    "web-vitals": "^1.0.1",
    "web-worker": "^1.0.0",
    "xstate": "^4.26.0"
  },
  "scripts": {
    "start": "concurrently \"npm:start:app\" \"npm:graphql:codegen\"",
    "start:app": "next",
    "build": "graphql-codegen && next build",
    "serve": "next start",
    "graphql:codegen": "graphql-codegen --watch",
    "postinstall": "patch-package && mkdir -p public/monaco-editor && cp -r node_modules/monaco-editor public",
    "cypress:open": "cypress open",
    "cypress:test": "cypress run",
    "cypress:record": "RECORD_ALL_CONTENT=1 RECORD_REPLAY_METADATA_FILE=$(mktemp) cypress run --browser 'Replay Firefox'",
    "jest": "jest",
    "changeset": "changeset",
    "e2e:wait": "wait-on http://localhost:3000",
    "e2e:run": "npm run e2e:wait && npm run cypress:test",
    "e2e:server": "npm run start & npm run e2e:run",
    "e2e:build": "npm run serve & npm run e2e:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/near-operation-file-preset": "^1.18.1",
    "@graphql-codegen/typed-document-node": "^1.18.6",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@testing-library/cypress": "^8.0.0",
    "@types/jest": "^27.0.1",
    "@types/lz-string": "^1.3.34",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.0",
    "cypress": "^8.0.0",
    "cypress-localstorage-commands": "^1.5.0",
    "cypress-real-events": "^1.5.0",
    "graphql": "^15.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^27.0.6",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.2",
    "sass": "^1.33.0",
    "typescript": "^4.3.5",
    "wait-on":"^6.0.1"
  }
}
